#include <stdio.h>
void main()
{
    char pass[20];
    printf("enter ur password honestly:");
    scanf("%s",pass);
    int len=strlen(pass);
    int l=0,u=0,d=0,s=0;
    if(len>7)
    {
        for(int i=0;i<len;i++)
    {
    if(isupper(pass[i]))
        u++;
    else if(islower(pass[i]))
        l++;
     else if(isdigit(pass[i]))
        d++;
     else
        s++;
    }
    if(l>0 && u>0 && s>0 && d>0)
        printf("STRONG PASSWORD");
    else
        printf("WEAK");
        }
    printf("WEAK DUE TO LESS CHARACTER");
    }
